{
  "name": "uglify",
  "description": "Uglify for ChiÏ‡",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodule/uglify.git"
  },
  "dependencies": {
    "uglify-js": "2.x.x"
  },
  "nodes": [
    {
      "name": "minify",
      "ns": "uglify",
      "description": "Minify a javascript source",
      "phrases": {
        "active": "Minifying source"
      },
      "ports": {
        "input": {
          "in": {
            "title": "Source",
            "type": "string"
          }
        },
        "output": {
          "out": {
            "type": "string",
            "title": "minified source"
          }
        }
      },
      "dependencies": {
        "npm": {
          "uglify-js": "2.x.x"
        }
      },
      "fn": "output.out = $.write('in', uglify_js.minify($.in, {fromString: true}));\n"
    }
  ]
}